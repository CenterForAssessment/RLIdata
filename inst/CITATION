         year         = "2022",
         note         = "R package version 16.0-0.0",
         url          = "https://github.com/CenterForAssessment/RLImatrices/",
         
         textVersion  = paste("Damian W. Betebenner and Adam R. Van Iwaarden (2022).",
                        "RLImatrices: Baseline coefficient matrices for SGPt analyses.",
                        "(R package version 16.0-0.0.",
                        "URL https://github.com/CenterForAssessment/RLImatrices/")
)
