citHeader("To cite package 'RLIdata' in publications use:")

citEntry(entry = "Manual",
         title        = "{RLIdata}: Baseline coefficient matrices for SGPt analyses.",
         author       = "Damian W. Betebenner and Adam Van Iwaarden",
         year         = "2015",
         note         = "R package version 1.0-0.0",
         url          = "https://github.com/CenterForAssessment/RLIdata/",
         
         textVersion  = paste("Damian W. Betebenner and Adam Van Iwaarden (2015).",
                        "RLIdata: Baseline coefficient matrices for SGPt analyses.",
                        "(R package version 1.0-0.0.",
                        "URL https://github.com/CenterForAssessment/RLIdata/")
)
